{
  "name": "@wenrenfangge/cesium-draw",
  "private": false,
  "version": "1.0.3",
  "type": "module",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "README-zh.md",
    "CHANGELOG.md"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wenrenfangge/cesium-draw"
  },
  "homepage": "https://github.com/wenrenfangge/cesium-draw/README.md",
  "keywords": [
    "cesium",
    "cesium-draw",
    "cesium-draw-irregular",
    "irregular",
    "不规则",
    "绘制",
    "绘制不规则"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc --emitDeclarationOnly && vite build",
    "preview": "vite preview",
    "build:lib": "tsc && vite build --target lib ./package/irregular/index.ts --name cesium-draw-irregular --dest cesium-draw-irregular",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.1",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-changelog-gitmoji-config": "^1.5.2",
    "rollup-plugin-copy": "^3.5.0",
    "terser": "^5.36.0",
    "typescript": "^5.5.3",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.4",
    "vitepress": "^1.4.1"
  },
  "dependencies": {
    "@turf/turf": "^7.1.0",
    "cesium": "^1.122.0",
    "element-plus": "^2.8.6"
  }
}
